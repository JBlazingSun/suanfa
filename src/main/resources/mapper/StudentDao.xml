<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatisplus.sql.tujie.dao.StudentDao">

    <resultMap type="com.example.mybatisplus.sql.tujie.entity.Student" id="StudentMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="name" column="name" jdbcType="VARCHAR"/>
        <result property="birth" column="birth" jdbcType="TIMESTAMP"/>
        <result property="gender" column="gender" jdbcType="VARCHAR"/>
    </resultMap>

    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">
        insert into tujie-sql.Student(name, birth, gender)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.name}, #{entity.birth}, #{entity.gender})
        </foreach>
    </insert>
    <insert id="insertOrUpdateBatch" keyProperty="id" useGeneratedKeys="true">
        insert into tujie-sql.Student(name, birth, gender)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.name}, #{entity.birth}, #{entity.gender})
        </foreach>
        on duplicate key update
        name = values(name) , birth = values(birth) , gender = values(gender)
    </insert>

</mapper>

