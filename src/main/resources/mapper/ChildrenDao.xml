<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatisplus.mybatisplus.userpagination.dao.ChildrenDao">

    <resultMap type="com.example.mybatisplus.mybatisplus.userpagination.entity.Children" id="ChildrenMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="name" column="name" jdbcType="VARCHAR"/>
        <result property="userPaginationId" column="user_pagination_id" jdbcType="INTEGER"/>
    </resultMap>

    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">
        insert into mp_sample.children(name, user_pagination_id)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.name}, #{entity.userPaginationId})
        </foreach>
    </insert>
    <insert id="insertOrUpdateBatch" keyProperty="id" useGeneratedKeys="true">
        insert into mp_sample.children(name, user_pagination_id)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.name}, #{entity.userPaginationId})
        </foreach>
        on duplicate key update
        name = values(name) , user_pagination_id = values(user_pagination_id)
    </insert>

</mapper>

